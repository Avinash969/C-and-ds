#include<stdio.h>
struct student
{
    int age;
    char name[10];
    float mark;
};

void main(){
    struct student s[5];
    for (int i=0;i<5;i++){
        scanf("%d",&s[i].age);
        scanf("%s",s[i].name);
        scanf("%f",&s[i].mark);
        
       
    }
    for(int i=0;i<5;i++){
       
     printf("\n%d",s[i].age);
      printf("\n%s",s[i].name);
       printf("\n%f",s[i].mark);
    }
}
